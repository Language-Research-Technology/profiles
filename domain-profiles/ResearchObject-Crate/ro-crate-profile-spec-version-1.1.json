                    "type": ["Person", "Organization"]
                }
            ]
        },
        "Person": {
            "definition": "inherit",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "https://schema.org/email",
                    "name": "email",
                    "multiple": true,
                    "type": ["Text"]
                },
                {
                    "id": "https://schema.org/affiliation",
                    "name": "affiliation",
                    "multiple": true,
                    "type": ["Organization"]
                },
                {
                    "id": "https://schema.org/contactPoint",
                    "name": "contactPoint",
                    "multiple": true,
                    "type": ["ContactPoint"]
                }
            ]
        }
    },
    "lookup": {}
