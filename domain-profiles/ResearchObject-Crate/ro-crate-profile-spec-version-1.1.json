{
    "metadata": {
        "name": "Research Object Crate Profile - Spec Version",
        "description": "A profile satisfying the requirements of the RO Crate Spec version 1.1",
        "version": 1.1,
        "warnMissingProperty": true,
        "keywords": ["RO Crate", "Spec Version 1.1"]
    },
    "layouts": {
        "Dataset": [
            {
                "name": "About",
                "description": "",
